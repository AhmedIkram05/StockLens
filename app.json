{
  "expo": {
    "name": "stocklens-app",
    "slug": "stocklens-app",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/StockLens_Logo_T.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/StockLens_Logo_T.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Use Face ID to quickly sign in to StockLens"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/StockLens_Logo_T.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/StockLens_Logo_T.png"
    },
    "plugins": [
      "expo-sqlite"
    ]
    ,
    "extra": {
      "OCR_SPACE_API_KEY": "K86345927288957"
    }
  }
}
